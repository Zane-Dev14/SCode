{
  "name": "scode",
  "displayName": "SCode Analyzer",
  "description": "Code analysis and visualization tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:scode.analyze"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "scode.analyze",
        "title": "Analyze Workspace"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "scode.analyze",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^6.11.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "gsap": "^3.12.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "three": "^0.152.2"
  }
}
